generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Chingu {
  id        String    @id @default(uuid())
  timestamp DateTime?

  gender      String?
  countryCode String?
  timezone    String?

  goal      String?
  goalOther String?

  source      String?
  sourceOther String?

  countryName String?

  soloProjectTier String?
  roleType        String?
  voyageRole      String?
  voyageSignup    String?
  voyageTier      String?

  @@index([countryCode])
  @@map("chingu")
}
